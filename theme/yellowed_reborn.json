                // Markdown
                "punctuation.definition.link.description.begin.markdown",
                "punctuation.definition.link.description.end.markdown",
                "punctuation.definition.link.title.begin.markdown",
                "punctuation.definition.link.title.end.markdown",

                // Ruby
                "support.function.kernel.ruby",

                // Yaml
                "entity.name.tag.yaml",

                // Zig
                "entity.name.type.zig",
                "support.function.builtin.zig"
            ],
            "settings": {
                "foreground": "#9b9b9b",
                "fontStyle": "bold"
            }
        },
        {
            "name": "White",
            "scope": [
                "source",
                "storage.modifier.import",
                "storage.modifier.package",
                "text",

                // EditorConfig
                "meta.section.editorconfig",

                // Csharp
                "meta.interpolation.cs",

                // Javascript
                "meta.embedded.line.js",

                // Json
                "support.type.property-name.json",

                // Markdown
                "markup.underline.link.image.markdown",
                "punctuation.definition.metadata.markdown",

                // Ruby
                "constant.language.symbol.hashkey.ruby"
            ],
            "settings": {
                "foreground": "#FFFFFF",
                "fontStyle": ""
            }
        },
        {
            "name": "White Bold",
            "scope": [
                // Go
                "entity.name.package.go",

                // Markdown
                "entity.name.section.markdown",
                "punctuation.definition.constant.markdown"
            ],
            "settings": {
                "foreground": "#FFFFFF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "White Italic",
            "scope": [
                "entity.name.variable.parameter",
                "variable.parameter",

                // Ruby
                "constant.other.symbol.hashkey.parameter.function.ruby"
            ],
            "settings": {
                "foreground": "#FFFFFF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "White Underline",
            "scope": [
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#FFFFFF",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Bold",
            "scope": [
                // Markdown
                "markup.bold.markdown"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Italic",
            "scope": [
                // Markdown
                "markup.italic.markdown",
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strikethrough",
            "scope": [
                // Markdown
                "markup.strikethrough.markdown"
            ],
            "settings": {
                "fontStyle": "strikethrough"
            }
        }
    ]
