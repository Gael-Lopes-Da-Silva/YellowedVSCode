    "tokenColors": [
        {
            "name": "Gray",
            "scope": [
                "comment",
                "emphasis",
                "unused.comment",
                "wildcard.comment"
            ],
            "settings": {
                "foreground": "#6b6b6b",
                "fontStyle": ""
            }
        },
        {
            "name": "Gray Bold",
            "scope": [
                "punctuation.definition.comment",
                "punctuation.definition.string.begin",
                "punctuation.definition.string.end",

                // Markdown
                "fenced_code.block.language.markdown",
                "markup.inline.raw.string.markdown",

                // Csharp
                "punctuation.definition.char.begin.cs",
                "punctuation.definition.char.end.cs",

                // Javascript
                "punctuation.definition.string.template.begin.js",
                "punctuation.definition.string.template.end.js",

                // Rust
                "punctuation.definition.char.rust",
                "punctuation.definition.string.rust"
            ],
            "settings": {
                "foreground": "#6b6b6b",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Green",
            "scope": [
                "string.template",
                "string",

                // Css
                "entity.other.attribute-name.pseudo-class.css",
                "entity.other.attribute-name.pseudo-element.css"
            ],
            "settings": {
                "foreground": "#378b1d",
                "fontStyle": ""
            }
        },
        {
            "name": "Purple",
            "scope": [
                "constant.character",
                "constant.language",
                "constant.regexp",

                // Csharp
                "punctuation.definition.interpolation.begin.cs",
                "punctuation.definition.interpolation.end.cs",

                // Javascript
                "punctuation.definition.template-expression.begin.js",
                "punctuation.definition.template-expression.end.js",

                // Ruby
                "punctuation.section.embedded.begin.ruby",
                "punctuation.section.embedded.end.ruby"
            ],
            "settings": {
                "foreground": "#ff80f4",
                "fontStyle": ""
            }
        },
        {
            "name": "Purple bold",
            "scope": [
                "constant.other"
            ],
            "settings": {
                "foreground": "#ff80f4",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Yellow",
            "scope": [
                "constant.numeric",
                "entity.name.tag",
                "keyword",
                "storage.type",

                // EditorConfig
                "punctuation.separator.key-value.editorconfig",

                // Html
                "punctuation.separator.key-value.html",

                // Lua
                "punctuation.definition.block.tag.ldoc",

                // Php
                "punctuation.section.embedded.begin.php",
                "punctuation.section.embedded.end.php",

                // Rust
                "entity.name.type.numeric.rust",
                "punctuation.brackets.attribute.rust",
                "punctuation.definition.attribute.rust",
                "storage.modifier.mut.rust"
            ],
            "settings": {
                "foreground": "#ffd900",
                "fontStyle": ""
            }
        },
        {
            "name": "Yellow Bold",
            "scope": [
                "keyword.control",
                "keyword.operator.constructor",
                "keyword.other",
                "storage.modifier",

                // C
                "storage.type.enum.c",
                "storage.type.struct.c",

                // Dart
                "keyword.declaration.dart",

                // Go
                "keyword.const.go",
                "keyword.function.go",
                "keyword.import.go",
                "keyword.package.go",
                "keyword.type.go",

                // Python
                "entity.name.function.decorator.python",
                "storage.type.class.python",
                "storage.type.function.python",

                // Javascript
                "storage.type.function.js",

                // Php
                "storage.type.class.php",
                "storage.type.function.php",

                // Markdown
                "markup.heading.setext.1.markdown",
                "markup.heading.setext.2.markdown",
                "meta.separator.markdown",
                "punctuation.definition.bold.markdown",
                "punctuation.definition.heading.markdown",
                "punctuation.definition.italic.markdown",
                "punctuation.definition.list.begin.markdown",
                "punctuation.definition.markdown",
                "punctuation.definition.quote.begin.markdown",
                "punctuation.definition.raw.markdown",
                "punctuation.definition.strikethrough.markdown",

                // Shell
                "storage.type.alias.shell",
                "storage.type.function.shell",

                // Zig
                "keyword.storage.zig",
                "storage.type.function.zig"
            ],
            "settings": {
                "foreground": "#ffd900",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Light Gray Bold",
            "scope": [
                "entity.name.function-call",
                "entity.name.function",

                // Css
                "meta.property-value.css",
                "support.constant.property-value.css",

