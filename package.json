{
    "name": "yellowed",
    "version": "2.1.0",
    "displayName": "Yellowed",
    "description": "A lemon like theme for vscode.",
    "publisher": "gael-lopes-da-silva",
    "icon": "resources/icon.png",
    "license": "LICENSE.md",
    "markdown": "standard",
    "engines": {
        "vscode": "^1.13.2"
    },
    "categories": [
        "Themes"
    ],
    "maintainers": [
        "Gael Lopes Da Silva"
    ],
    "scripts": {
        "build": "node ./scripts/build.js",
        "lint": "node ./scripts/lint.js",
        "package": "vsce package -o ./build/",
        "vscode:prepublish": "npm run build",
        "vsce-publish": "vsce publish"
    },
    "contributes": {
        "themes": [
            {
                "label": "Yellowed",
                "uiTheme": "vs-dark",
                "path": "./theme/yellowed.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Gael-Lopes-Da-Silva/Yellowed"
    },
    "devDependencies": {
        "js-yaml": "^4.1.0",
        "vsce": "^1.97.0"
    },
    "galleryBanner": {
        "color": "#ffd900",
        "theme": "dark"
    },
    "homepage": "https://github.com/Gael-Lopes-Da-Silva/Yellowed",
    "keywords": [
        "amogus",
        "yellowed",
        "yellow",
        "dark",
        "material",
        "minimalist",
        "minimal",
        "gruber",
        "jaune",
        "golden",
        "gold"
    ]
}
